{
	"name": "Chrome-Dino-Game",
	"build": {
        "dockerfile": "Dockerfile"
    },
    "runArgs": [
        "--cap-add=SYS_PTRACE",
        "--security-opt",
        "seccomp=unconfined"
    ],
	"customizations": {
		"vscode": {
			"extensions": [
				"kisstkondoros.vscode-gutter-preview",
				"golang.go",
				"mhutchie.git-graph",
				"rust-lang.rust-analyzer"
			]
		}
	}
}
